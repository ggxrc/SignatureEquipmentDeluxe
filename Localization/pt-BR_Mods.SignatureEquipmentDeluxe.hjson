# Signature Equipment Deluxe - English Localization

Mods: {
	SignatureEquipmentDeluxe: {
		Keybinds.OpenSignatureUI: Abrir Interface de Equipamentos Assinados

		ItemName: {
			SignatureManagementTome: Tomo de Gerenciamento de Assinaturas
			XPBooster: Impulsionador de XP
		}

		ItemTooltip: {
			SignatureManagementTome:
				'''
				Abre a interface de Gerenciamento de Equipamentos Assinados
				Visualize e gerencie todos os seus equipamentos assinados
				'''
			XPBooster:
				'''
				Aumenta permanentemente o ganho de XP em 10%
				O efeito acumula com múltiplos usos
				'''
		}

		Common: {
			SignedEquipment: ⭐ Equipamento Assinado ⭐
			Level: Nível {0}
			Prestige: Prestígio {0}
			XP: XP: {0}/{1} ({2}%)
			MaxLevel: NÍVEL MÁXIMO
			Bonuses: +{0}% Dano, +{1}% Crítico, +{2}% Velocidade
			Stats: Kills: {0} | Dano Total: {1} | Tempo: {2}
			PrestigeAvailable: ⚡ PRESTÍGIO DISPONÍVEL! ⚡
			CanSign: Clique com botão direito para vincular como equipamento assinado
			Signed: "[c/FFD700:{0}] foi vinculado como seu equipamento assinado!"
			Unsigned: "{0} foi desvinculado!"
			PrestigeComplete: "{0} alcançou Prestígio {1}!"
			LevelUp: Seu equipamento subiu para o nível {0}!
			XPGained: +{0} XP
			XPMultiplierIncreased: Multiplicador de XP aumentado para {0}!
			Dodge: ⚡ ESQUIVA! ⚡
		}

		UI: {
			Title: ⭐ Equipamentos Assinados ⭐
			TotalEquipment: Total: {0} equipamentos assinados
			NoEquipment: Nenhum equipamento assinado ainda
			HelpText:
				'''
				Clique com o botão direito em qualquer arma
				ou armadura para vinculá-lo!
				Acessórios fornecem XP para armas equipadas!
				'''
			Close: X
			Remove: ✖
			Prestige: ⚡
		}

		Abilities: {
			Lifesteal: {
				Name: Roubo de Vida (Nv 25+)
				Description: Acertos críticos curam {0}% do dano causado
			}

			ExtraProjectile: {
				Name: Tiro Duplo (Nv 50+)
				Description: 20% de chance de disparar um projétil adicional
			}

			DamageAura: {
				Name: Aura de Dano (Nv 75+)
				Description: Causa dano periodicamente em inimigos próximos
			}

			Dodge: {
				Name: Esquiva (Prestígio 1+)
				Description: "{0}% de chance de esquivar completamente de ataques"
			}

			ExplosionOnKill: {
				Name: Explosão (Prestígio 3+)
				Description: Kills criam explosões que causam dano
			}

			FastRegen: {
				Name: Regeneração Rápida (Prestígio 5+)
				Description: Regeneração de vida muito aumentada
			}
		}
	}
}

